{
  "name": "github_clone",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "prepare": "husky install",
    "pods": "cd ios && bundle exec pod install && cd ..",
    "reinstall": "rm -rf node_modules && yarn",
    "rebuild": "yarn reinstall && rm -rf ./ios/Pods && yarn pods",
    "postinstall": "patch-package",
    "lint-staged": "lint-staged",
    "images": "node scripts/images.js",
    "fonts": "node scripts/fonts.js",
    "rename": "react-native-rename",
    "test": "jest",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write \"**/*.{js,jsx}\"",
    "format:json": "prettier --write \"**/*.json\"",
    "format:md": "prettier --write \"**/*.md\"",
    "format:ts": "prettier --write \"**/*.{ts,tsx}\"",
    "check-format": "npx prettier -c \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "lint": "npm-run-all lint:*",
    "lint:ts": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:tsc": "yarn tsc -p tsconfig.json",
    "lint:styled": "stylelint \"**/*.styles.ts\"",
    "lint-fix": "yarn lint:ts --fix",
    "lint-styled-fix": "yarn lint:styled --fix",
    "android-gems": "cd android && bundle install && cd ..",
    "ios-gems": "cd ios && bundle install && cd ..",
    "android-beta": "cd android && fastlane beta && cd ..",
    "ios-beta": "cd ios && fastlane beta && cd ..",
    "stylelint-check": "stylelint-config-prettier-check"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.4",
    "@react-navigation/bottom-tabs": "^6.0.0-next.18",
    "@react-navigation/native": "^6.0.0-next.13",
    "@react-navigation/stack": "^6.0.0-next.17",
    "@reduxjs/toolkit": "^1.5.1",
    "apisauce": "^2.1.1",
    "axios-retry": "^3.1.9",
    "lodash": "^4.17.21",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-config": "^1.4.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-reanimated": "^2.1.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.2.0",
    "react-native-splash-screen": "^3.2.0",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "remote-redux-devtools": "^0.5.16",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@redux-saga/is": "^1.1.2",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/react-native": "^0.64.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux-logger": "^3.0.8",
    "@types/styled-components-react-native": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "metro-react-native-babel-preset": "^0.64.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "react-native-flipper": "^0.75.1",
    "react-native-rename": "^2.8.1",
    "react-test-renderer": "17.0.1",
    "reactotron-apisauce": "^3.0.0",
    "reactotron-core-client": "^2.8.10",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "reactotron-redux-saga": "^4.2.3",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-react-native-styled-components": "^0.3.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-react-native": "^2.2.0",
    "typescript": "^4.2.0"
  },
  "resolutions": {
    "@react-navigation/elements": "^1.0.0-next.17",
    "@types/react": "^17",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "apisauce": "^2.1.1",
    "axios": "^0.21.1",
    "react-native-flipper": "^0.75.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "yarn lint-fix"
    ],
    "*.md": [
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.styles.ts": [
      "yarn lint:styled"
    ]
  }
}
